) => {\n        fetch(url + '/products')\n        .then((res) => res.json())\n        .then((result) => {\n            cb(result);\n        });\n    },\n    buyProducts: (data, cb) => {\n        setTimeout(() => {\n            fetch(url + '/orders', {method: 'POST', body: JSON.stringify(data)})\n            .then((res) => res.json())\n            .then((result) => {\n                cb(result);\n            });\n        }, 2000) //needed to simulate some delay\n\n        \n    }\n}","/Users/zahraaalmasslawi/localCode/reactProjects/shopping/src/reducers/index.js",[],"/Users/zahraaalmasslawi/localCode/reactProjects/shopping/src/components/ProductItem/ProductItem.js",[],"/Users/zahraaalmasslawi/localCode/reactProjects/shopping/src/components/OrderItem/OrderItem.js",[],"/Users/zahraaalmasslawi/localCode/reactProjects/shopping/src/components/ProductWrapper/ProductWrapper.js",["85"],"import React from 'react';\n\nfunction ProductWrapper(props) {\n\n    function inventory() {\n        if (props.product.inventory > 0) \n            return <p>{props.product.inventory} available</p>;\n        else \n            return <p>Sold out</p>;\n    }\n\n    return(\n        <div className=\"productItem\">\n\n            <h5>{props.product.name}</h5>\n            <h6> ${props.product.price}</h6>\n            {inventory()}\n            <img src={props.product.image} />\n            {props.children}\n        </div>\n    );\n}\n\nexport default ProductWrapper;",{"ruleId":"86","replacedBy":"87"},{"ruleId":"88","replacedBy":"89"},{"ruleId":"90","severity":1,"message":"91","line":1,"column":8,"nodeType":"92","messageId":"93","endLine":1,"endColumn":12},{"ruleId":"86","replacedBy":"94"},{"ruleId":"88","replacedBy":"95"},{"ruleId":"86","replacedBy":"96"},{"ruleId":"88","replacedBy":"97"},{"ruleId":"86","replacedBy":"98"},{"ruleId":"88","replacedBy":"99"},{"ruleId":"90","severity":1,"message":"100","line":1,"column":28,"nodeType":"92","messageId":"93","endLine":1,"endColumn":36},{"ruleId":"90",